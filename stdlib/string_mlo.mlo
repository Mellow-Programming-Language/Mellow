
import std.core;
import std.io;
import std.conv;

func join(strs: []string, joiner: string): string {
    if (strs.length == 0) {
        return "";
    }
    else if (strs.length == 1) {
        return strs[0];
    }
    newstr := strs[0];
    foreach (s; strs[1..]) {
        newstr = newstr ~ joiner ~ s;
    }
    return newstr;
}
